[dir="rtl"] .after-hours-form-container .glue-c-modal__dialog .glue-c-modal__content .captcha-error-text { transform: translate(-130px, -20px); }

.mdc-select__menu { z-index: 9999; }

.mdc-select__menu .mdc-list-item--selected { background-color: rgba(66, 133, 244, 0.3); }

.mdc-select__menu :not(.mdc-list--non-interactive) > :not(.mdc-list-item--disabled).mdc-list-item--selected::after, .mdc-select__menu :not(.mdc-list--non-interactive) > :not(.mdc-list-item--disabled).mdc-list-item--selected::before { background-color: rgba(255, 255, 255, 0); }

.no-dsa .endcap-phone-number, .no-dsa .eto-number, .no-dsa .eto_availability, .no-dsa .eto_copy_block, .no-dsa .footer-phone-terms, .no-dsa .header-phone-number, .no-dsa .hero-eto-number, .no-dsa .idle-widget { display: none !important; }

.in-hours .after-hours-form-container, .in-hours .contact-us-cta { display: none !important; }

.out-of-hours .header-phone-number, .out-of-hours .idle-widget { display: none !important; }
